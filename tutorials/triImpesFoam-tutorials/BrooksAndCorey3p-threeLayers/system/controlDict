FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     triImpesFoam;

startFrom       latestTime;

startTime       0.0;

stopAt          endTime;

endTime         20000;//2e6;

deltaT          1e-7;

writeControl    adjustableRunTime;

writeInterval   2000;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

CFL            Todd;

maxCo		0.75;

prefactor       0.5;

dSmax           0.005;

maxDeltaT	1000;//e-3;

