FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
	volScalarFieldValue Sw 2e-3
        volScalarFieldValue So 0.1e-3
	volScalarFieldValue Winj 0
	volScalarFieldValue Wext 0
);



regions
(
    // injection
    boxToCell
   {
     box (0 0 0) (0.1 0.5 0.1);
     fieldValues
        (
           volScalarFieldValue Sw 0.5
           volScalarFieldValue So 0.45        
        );
   }   
      
     

);



